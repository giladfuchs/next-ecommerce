{
  "name": "next-ecommerce-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend for Next.js e-commerce platform",
  "author": "GILAD",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "nextjs",
    "ecommerce",
    "typescript",
    "storefront",
    "admin-dashboard"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/giladfuchs/next-ecommerce"
  },
  "scripts": {
    "clean": "rm -rf .next",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:webpack": "next dev --webpack",
    "dev": "next dev",
    "build": "next build --webpack",
    "build:analyze": "ANALYZE=true next build --webpack",
    "start": "next start",
    "format": "npx prettier --write .",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "NEXT_PUBLIC_PORT=4013 NODE_ENV=test pnpm dev:webpack & sleep 2 & pnpm exec playwright test --headed",
    "test:file": "NEXT_PUBLIC_PORT=4013 NODE_ENV=test pnpm dev:webpack & sleep 2 & pnpm exec playwright test e2e/order.spec.ts --headed",
    "codegen:record": "NEXT_PUBLIC_PORT=4010 NODE_ENV=test pnpm dev:webpack & sleep 2 && pnpm exec playwright codegen http://localhost:3000"
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@mui/icons-material": "7.2.0",
    "@mui/material": "7.2.0",
    "@mui/material-nextjs": "^7.3.6",
    "@mui/system": "7.2.0",
    "@mui/x-tree-view": "8.10.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ag-grid-community": "34.1.1",
    "ag-grid-react": "34.1.1",
    "formik": "^2.4.6",
    "next": "16.0.7",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-intl": "7.1.11",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "sonner": "^2.0.3",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@next/bundle-analyzer": "^16.0.7",
    "@next/eslint-plugin-next": "^16.0.7",
    "@playwright/test": "1.54.2",
    "@types/node": "22.14.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "prettier": "3.6.2",
    "sass": "^1.86.3",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.48.1",
    "webpack": "^5.103.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all"
  ],
  "packageManager": "pnpm@10.8.1"
}

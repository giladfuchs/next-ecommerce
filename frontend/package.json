{
  "name": "next-ecommerce-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend for Next.js e-commerce platform",
  "author": "GILAD",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "nextjs",
    "ecommerce",
    "typescript",
    "storefront",
    "admin-dashboard"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/giladfuchs/next-ecommerce"
  },
  "scripts": {
    "clean": "rm -rf .next",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:webpack": "next dev --webpack",
    "dev": "next dev",
    "build": "next build --webpack",
    "build:analyze": "ANALYZE=true next build --webpack",
    "start": "next start",
    "format": "npx prettier --write .",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "NEXT_PUBLIC_PORT=4013 NODE_ENV=test pnpm dev:webpack & sleep 2 & pnpm exec playwright test --headed",
    "test:file": "NEXT_PUBLIC_PORT=4013 NODE_ENV=test pnpm dev:webpack & sleep 2 & pnpm exec playwright test e2e/order.spec.ts --headed",
    "codegen:record": "NEXT_PUBLIC_PORT=4010 NODE_ENV=test pnpm dev:webpack & sleep 2 && pnpm exec playwright codegen http://localhost:3000"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.3.8",
    "@mui/material": "7.3.8",
    "@mui/material-nextjs": "^7.3.8",
    "@mui/system": "7.3.8",
    "@mui/x-tree-view": "8.27.2",
    "@reduxjs/toolkit": "^2.11.2",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "ag-grid-community": "35.1.0",
    "ag-grid-react": "35.1.0",
    "formik": "^2.4.9",
    "next": "16.1.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-intl": "8.1.3",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "sonner": "^2.0.7",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@next/bundle-analyzer": "^16.1.6",
    "@next/eslint-plugin-next": "^16.1.6",
    "@playwright/test": "1.58.2",
    "@types/node": "25.3.1",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.39.0",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^17.3.0",
    "prettier": "3.8.1",
    "sass": "^1.97.3",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.56.1",
    "webpack": "^5.105.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all"
  ],
  "packageManager": "pnpm@10.8.1"
}
